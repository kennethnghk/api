{
  "name": "learn-docker",
  "version": "1.0.0",
  "main": "server.js",
  "repository": "https://github.com/kennethnghk/learn-docker",
  "author": "Kenneth Ng",
  "license": "MIT",
  "scripts": {
    "start": "node ./src/server.js",
    "format": "prettier-standard '**/*.js'",
    "pre-commit": "lint-staged",
    "fixcode": "standard --fix"
  },
  "standard": {
    "globals": [
      "__DEV__",
      "fetch"
    ]
  },
  "lint-staged": {
    "linters": {
      "**/*.js": [
        "prettier-standard",
        "standard --fix",
        "git add"
      ]
    }
  },
  "dependencies": {
    "express": "^4.16.3",
    "mqtt": "^2.18.2",
    "rhea": "^0.2.15",
    "swagger-combine": "^0.18.1",
    "swagger-tools": "^0.10.3"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier-standard": "^8.0.1",
    "standard": "^11.0.1"
  }
}
